<p id="notice"><%= notice %></p>

<h1>Medical Capacity Management</h1>

<%= button_to 'New Medical Capacity Record', new_medicalsupply_path, method: :get %>

<p>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Item</th>
      <th>Quantity</th>
      <th>Unit</th>
      <th>Province</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medicalsupplies.each do |medicalsupply| %>
      <tr>
        <td><%= medicalsupply.date %></td>
        <td><%= medicalsupply.item %></td>
        <td><%= medicalsupply.quantity %></td>
        <td><%= medicalsupply.unit %></td>
        <td><%= medicalsupply.province %></td>
        <td><%= link_to 'Show', medicalsupply %></td>
        <td><%= link_to 'Edit', edit_medicalsupply_path(medicalsupply) %></td>
        <td><%= link_to 'Del', medicalsupply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p>

<%= button_to 'New Medical Capacity Record', new_medicalsupply_path, method: :get %>
